<?xml version="1.0" encoding="utf-8"?>
<install type="module" version="1.5.0" position="left" method="upgrade" client="site">
	<name>Tienda - My Orders</name>
	<author>Dioscouri</author>
	<authorEmail>info@dioscouri.com</authorEmail>
	<authorUrl>http://www.dioscouri.com</authorUrl>
	<version>0.10.1</version>
	<creationDate>October 2011</creationDate>
	<copyright>2012 Dioscouri.com</copyright>
	<license>GNU General Public License</license>
	<description>MOD_TIENDA_MY_ORDERITEMS_MODULEDESC</description>
	<files>
		<filename module="mod_tienda_my_orders">mod_tienda_my_orders.php</filename>
		<filename>helper.php</filename>
		<filename>index.html</filename>
		<folder>tmpl</folder>
	</files>
	<languages>
		<language tag="en-GB">languages/en-GB.mod_tienda_my_orders.ini</language>
	</languages>
	<config>
		<fields name="params">
			<fieldset name="basic">
				<field type="spacer" />
				<field name="display_id" type="list" default="1" label="MOD_TIENDA_MY_ORDERS_SHOW_ID" description="MOD_TIENDA_MY_ORDERS_SHOW_ID_DESC">
					<option value="1">ENABLED</option>
					<option value="0">DISABLED</option>
				</field>
				<field name="display_date" type="list" default="1" label="MOD_TIENDA_MY_ORDERS_SHOW_DATE" description="MOD_TIENDA_MY_ORDERS_SHOW_DATE_DESC">
					<option value="1">ENABLED</option>
					<option value="0">DISABLED</option>
				</field>
				<field name="display_image_link" type="list" default="1" label="MOD_TIENDA_MY_ORDERS_LINK_IMAGE" description="MOD_TIENDA_MY_ORDERS_LINK_IMAGE_DESC">
					<option value="1">ENABLED</option>
					<option value="0">DISABLED</option>
				</field>
				<field name="display_amount" type="list" default="1" label="MOD_TIENDA_MY_ORDERS_SHOW_AMOUNT" description="MOD_TIENDA_MY_ORDERS_SHOW_AMOUNT_DESC">
					<option value="1">ENABLED</option>
					<option value="0">DISABLED</option>
				</field>
				<field name="display_state" type="list" default="1" label="MOD_TIENDA_MY_ORDERS_SHOW_STATE" description="MOD_TIENDA_MY_ORDERS_SHOW_STATE_DESC">
					<option value="1">ENABLED</option>
					<option value="0">DISABLED</option>
				</field>
				<field name="max_number" type="text" default="5" label="MOD_TIENDA_MY_ORDERS_MAX_NUMBER" description="MOD_TIENDA_MY_ORDERS_MAX_NUMBER_DESC" />
				<field type="spacer" />
				<field name="null_text" type="text" size="50" default="No Orders Found" label="MOD_TIENDA_MY_ORDERS_NO_ITEMS_TEXT" description="MOD_TIENDA_MY_ORDERS_NO_ITEMS_TEXT_DESC"></field>
			</fieldset>
			<fieldset
				name="advanced">
				<field
					name="layout"
					type="modulelayout"
					label="JFIELD_ALT_LAYOUT_LABEL"
					description="JFIELD_ALT_MODULE_LAYOUT_DESC" />
				<field
					name="moduleclass_sfx"
					type="text"
					label="COM_MODULES_FIELD_MODULECLASS_SFX_LABEL"
					description="COM_MODULES_FIELD_MODULECLASS_SFX_DESC" />
				<field
					name="cache"
					type="list"
					default="1"
					label="COM_MODULES_FIELD_CACHING_LABEL"
					description="COM_MODULES_FIELD_CACHING_DESC">
					<option
						value="1">JGLOBAL_USE_GLOBAL</option>
					<option
						value="0">COM_MODULES_FIELD_VALUE_NOCACHING</option>
				</field>
				<field
					name="cache_time"
					type="text"
					default="900"
					label="COM_MODULES_FIELD_CACHE_TIME_LABEL"
					description="COM_MODULES_FIELD_CACHE_TIME_DESC" />
				<field
					name="cachemode"
					type="hidden"
					default="itemid">
					<option
						value="itemid"></option>
				</field>
			</fieldset>
		</fields>
	</config>
	<params>
		<param type="spacer" />
		<param name="display_id" type="list" default="1" label="MOD_TIENDA_MY_ORDERS_SHOW_ID" description="MOD_TIENDA_MY_ORDERS_SHOW_ID_DESC">
			<option value="1">ENABLED</option>
			<option value="0">DISABLED</option>
		</param>
		<param name="display_date" type="list" default="1" label="MOD_TIENDA_MY_ORDERS_SHOW_DATE" description="MOD_TIENDA_MY_ORDERS_SHOW_DATE_DESC">
			<option value="1">ENABLED</option>
			<option value="0">DISABLED</option>
		</param>
		<param name="display_image_link" type="list" default="1" label="MOD_TIENDA_MY_ORDERS_LINK_IMAGE" description="MOD_TIENDA_MY_ORDERS_LINK_IMAGE_DESC">
			<option value="1">ENABLED</option>
			<option value="0">DISABLED</option>
		</param>
		<param name="display_amount" type="list" default="1" label="MOD_TIENDA_MY_ORDERS_SHOW_AMOUNT" description="MOD_TIENDA_MY_ORDERS_SHOW_AMOUNT_DESC">
			<option value="1">ENABLED</option>
			<option value="0">DISABLED</option>
		</param>
		<param name="display_state" type="list" default="1" label="MOD_TIENDA_MY_ORDERS_SHOW_STATE" description="MOD_TIENDA_MY_ORDERS_SHOW_STATE_DESC">
			<option value="1">ENABLED</option>
			<option value="0">DISABLED</option>
		</param>
		<param name="max_number" type="text" default="5" label="MOD_TIENDA_MY_ORDERS_MAX_NUMBER" description="MOD_TIENDA_MY_ORDERS_MAX_NUMBER_DESC" />
		<param type="spacer" />
		<param name="moduleclass_sfx" type="text" default="" label="COM_MODULES_FIELD_MODULECLASS_SFX_LABEL" description="COM_MODULES_FIELD_MODULECLASS_SFX_DESC" />
		<param type="spacer" />
		<param name="null_text" type="text" size="50" default="No Orders Found" label="MOD_TIENDA_MY_ORDERS_NO_ITEMS_TEXT" description="MOD_TIENDA_MY_ORDERS_NO_ITEMS_TEXT_DESC"></param>
	</params>
</install>